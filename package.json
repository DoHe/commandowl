{
  "name": "commandowl",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10"
  },
  "scripts": {
    "heroku-postbuild": "VUE_APP_MONGODB_URI=$MONGODB_URI vue-cli-service ssr:build",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "ssr:build": "vue-cli-service ssr:build",
    "ssr:serve": "vue-cli-service ssr:serve",
    "ssr:start": "cross-env NODE_ENV=production vue-cli-service ssr:serve --mode production"
  },
  "dependencies": {
    "bulma": "0.7.4",
    "eslint-import-resolver-webpack": "0.11.0",
    "mongodb": "3.1.13",
    "vue": "2.6.7",
    "vue-router": "3.0.2",
    "vue-server-renderer": "2.6.7",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@akryum/vue-cli-plugin-ssr": "0.5.0",
    "@vue/cli-plugin-babel": "3.4.1",
    "@vue/cli-plugin-eslint": "3.4.1",
    "@vue/cli-service": "3.4.1",
    "babel-eslint": "10.0.1",
    "eslint": "5.14.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-vue": "5.2.2",
    "node-sass": "4.11.0",
    "sass-loader": "7.1.0",
    "vue-template-compiler": "2.6.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}