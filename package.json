{
  "name": "commandowl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "heroku-postbuild": "VUE_APP_MONGODB_URI=$MONGODB_URI vue-cli-service ssr:build",
    "ssr:build": "vue-cli-service ssr:build",
    "ssr:serve": "vue-cli-service ssr:serve",
    "ssr:start": "cross-env NODE_ENV=production vue-cli-service ssr:serve --mode production",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "bulma": "0.7.5",
    "eslint-import-resolver-webpack": "0.11.1",
    "express": "4.17.1",
    "mongodb": "3.2.6",
    "vue": "2.6.10",
    "vue-router": "3.0.6",
    "vue-server-renderer": "2.6.10",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@akryum/vue-cli-plugin-ssr": "0.5.0",
    "@vue/cli-plugin-babel": "3.8.0",
    "@vue/cli-plugin-eslint": "3.8.0",
    "@vue/cli-plugin-unit-jest": "3.8.0",
    "@vue/cli-service": "3.8.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-vue": "5.2.2",
    "node-sass": "4.12.0",
    "sass-loader": "7.1.0",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "10"
  }
}
